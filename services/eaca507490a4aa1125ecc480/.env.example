# LangSmith SDK Environment Variables
# Copy this file to .env and fill in your values

# Required: Enable tracing
LANGSMITH_TRACING=true

# Required: API key from https://smith.langchain.com/settings
LANGSMITH_API_KEY=your_api_key_here

# Optional: API endpoint (defaults to https://api.smith.langchain.com)
LANGSMITH_ENDPOINT=https://api.smith.langchain.com

# Optional: Project name (defaults to "default")
LANGSMITH_PROJECT=default

# Optional: Workspace ID for org-scoped keys
# LANGSMITH_WORKSPACE_ID=

# Optional: Sampling rate 0-1
# LANGSMITH_TRACING_SAMPLING_RATE=

# Optional: Legacy fallback API key
# LANGCHAIN_API_KEY=