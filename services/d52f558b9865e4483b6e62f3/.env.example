# LLamaTuner Environment Configuration
# HuggingFace Hub endpoint (defaults to https://huggingface.co)
HF_ENDPOINT=

# Use ModelScope instead of HuggingFace (set to "true" or "1" to enable)
USE_MODELSCOPE_HUB=

# Disable version compatibility checks (set to "true" or "1" to enable)
DISABLE_VERSION_CHECK=

# Force use of torchrun for distributed training (set to "true" or "1" to enable)
FORCE_TORCHRUN=

# Distributed Training Configuration
MASTER_ADDR=127.0.0.1
MASTER_PORT=29500
NNODES=1
RANK=0
LOCAL_RANK=0
WORLD_SIZE=1
NPROC_PER_NODE=1